[req]
default_bits       = 2048
prompt             = no
default_md         = sha256
req_extensions     = req_ext
distinguished_name = dn

[dn]
C  = MY
ST = State
L  = City
O  = Org
OU = IT
CN = 192.168.100.140

[req_ext]
subjectAltName = @alt_names

[alt_names]
IP.1 = 192.168.100.140
DNS.1 = localhost
